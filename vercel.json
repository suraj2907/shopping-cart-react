{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "rewrites": [{ "source": "/((?!.*\\..*).*)", "destination": "/index.html" }],
  "env": {
    "NODE_VERSION": "18.20.6"
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.20.6"
    }
  }
}
